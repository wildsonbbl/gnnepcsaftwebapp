{
  "family": "gnnepcsaft-app",
  "containerDefinitions": [
    {
      "name": "gnnepcsaft-app",
      "image": "wildsonbbl/gnnepcsaft:v2",
      "cpu": 1024,
      "memory": 512,
      "memoryReservation": 512,
      "portMappings": [
        {
          "name": "webapp-8000-tcp",
          "containerPort": 8000,
          "hostPort": 8000,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "DJANGO_ALLOWED_HOSTS",
          "value": "*"
        },
        {
          "name": "DB_PORT",
          "value": "5432"
        },
        {
          "name": "DB_USER",
          "value": "masteruser"
        },
        {
          "name": "DJANGO_SECRET_KEY",
          "value": "J3jmV6Pv7Y5ECnBKR8kMu2N9SzywHXaAdpbeDUtLQhWrq4Zcgf"
        },
        {
          "name": "DB_NAME",
          "value": "postgres"
        },
        {
          "name": "DB_HOST",
          "value": "gnnepcsaft-db.c3gbth4232ai.us-east-1.rds.amazonaws.com"
        },
        {
          "name": "DOMAIN_NAME",
          "value": "gnnepcsaft.online"
        },
        {
          "name": "DB_PASSWORD",
          "value": "buppf4hHf5dHjCLJ1KDd"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/webapp",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      }
    }
  ],
  "taskRoleArn": "arn:aws:iam::375758611853:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::375758611853:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["EC2"],
  "cpu": "1946",
  "memory": "819",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
